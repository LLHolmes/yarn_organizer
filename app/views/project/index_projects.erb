<h1>Your Projects</h1>
<h3>In Progress:</h3>
  <% @wip.each do |project| %>
  <div>
    <a href="/project/<%= project.id %>"><%= project.name %></a>
  </div>
  <% end %>

<h3>Upcoming:</h3>
  <% @upcoming.each do |project| %>
  <div>
    <a href="/project/<%= project.id %>"><%= project.name %></a>
  </div>
  <% end %>

<h3>Finished:</h3>
  <% @finished.each do |project| %>
  <div>
    <a href="/project/<%= project.id %>"><%= project.name %></a>
  </div>
  <% end %>

<p><a href="/project/new">New Project</a></p>
